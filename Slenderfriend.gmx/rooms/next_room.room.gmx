  <code>show_debug_message("Creating Next Room");

randomize();

//create a random sized room between 400 x 400 and 2000 x 2000
r_height = floor(random_range(400,2000));
r_width = floor(random_range(400,2000));


friends = floor(random_range(10,15));
rx = 0;
ry = 0;

instance_create(0,0,tile_drawer);

color[0] = c_blue;
color[1] = c_fuchsia;
color[2] = c_green;

tile_drawer.sprite_color = color[floor(random_range(0,3))];

do
{
    do
    {
        rx = random(room_width);
        ry = random(room_height);
    }
    until (place_free(rx,ry));
    
    friends -= 1;
    
    which_friend = floor(random_range(0,global.friend_range));
    switch(which_friend)
    {
        case 0:
        instance_create (rx, ry, Normal);
        break;
        
        case 1:
        instance_create (rx, ry, Mutton);
        break;
        
        case 2:
        instance_create (rx, ry, Clingy);
        break;
    }
}
until (friends == 0);

room_persistent = true;</code>
