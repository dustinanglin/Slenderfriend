            <kind>1</kind>
            <string>if(!is_mutton_bustin)
{

//get scared
if(distance_to_object(Slenderfriend) &lt; 30)
{
    is_freaked_out = true;
}

//get not scared
if(distance_to_object(Slenderfriend) &gt; 300  &amp;&amp; !moving)
{
    is_freaked_out = false;
    moving = true;
    at_point = true;
    chill = false;
}

if (is_freaked_out)
{
    draw_set_color(c_black);
    draw_text(x+10, y+10, "AHHHHHHHH!!!!");
    move_towards_point(Slenderfriend.x, Slenderfriend.y,-1 * friend_speed);
    moving = false;
}

if (moving)
{

    if (distance_to_point(rx, ry) &lt; 2)
    {
        at_point = true;
    }
    
    if (at_point)
    {
        randomize();
        draw_text(x + 20, y + 20, "At point");
        rx = floor(random(room_width));
        ry = floor(random(room_height));
        at_point = false;
        alarm[1] = room_speed * chill_time;
    }
    if (!chill)
        move_towards_point(rx,ry,1);
    if (chill)
        move_towards_point(0,0,0);
}

if (distance_to_object(Slenderfriend) &lt; 10)
{
    draw_capture = true;
}
else
{
    draw_capture = false;
}

if (draw_capture &amp;&amp; keyboard_check(ord("R")))
{
    instance_destroy();
    score_keeper.self_esteem += score_keeper.friend_add;
    score_keeper.friend_count += 1;
}
}

if (is_punched)
{
    is_punched = false;
    
    is_mutton_bustin = true;
    mutton_timer = 10;
    show_debug_message("Bust some mutton!");
    Slenderfriend.ride_along = true;
}


if (is_mutton_bustin)
{
    if (mutton_timer == 10)
    {
        //goooooooooo! :)
        
        //recent ai path logic
        move_towards_point(0,0,0);
        alarm[2] = 1;
    }
    
    if (mutton_timer &lt;= 0)
    {
        is_mutton_bustin = false;
        Slenderfriend.ride_along = false;
        mutton_timer = 10;
    }
    
    x += mhspeed;
    y += mvspeed;   
    
    mutton_timer -= 1 / room_speed;
    score_keeper.friction_value = mutton_friction;
}


//stay in bounds
if (x &gt;= room_width - sprite_width)
    x = room_width - sprite_width;

if (x &lt;= 0)
    x = 0;

if (y &gt;= room_height - sprite_height)
    y = room_height - sprite_height;

if (y &lt;= 0)
    y = 0;
    

//regenerate health
if (is_freaked_out &amp;&amp; my_health &lt;= 5)
    my_health += .5 / room_speed;
else if (my_health &lt;= 5)
    my_health += 1 / room_speed;

if (my_health &lt;= 0)
{
    instance_destroy();
    score_keeper.friend_count += 1;
    score_keeper.increment = true;
    score_keeper.amount = global.friend_value[0];
}



if (mutton_friction &gt;= 3)
{
    
    Slenderfriend.ride_along = false;
    score_keeper.friend_count += 1;
    score_keeper.increment = true;
    score_keeper.amount = global.friend_value[1];
    
    instance_destroy();
}

if (keyboard_check_pressed(ord("V")))
{
    friend_max += 1;
}

friend_speed = 2 + logn(10,score_keeper.difficulty);
if (friend_speed &gt;= friend_max)
    friend_speed = friend_max - .1;

